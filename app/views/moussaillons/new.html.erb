<h1>Creer un Moussaillon</h1>

<%= form_for @moussaillon do |f| %>
  <div class="form_group">
    <label>Pseudo du Moussaillon:</label>
    <%= f.text_field :pseudo, class: 'form-control' %>
  </br>
    <label>Email du Moussaillon:</label>
    <%= f.text_field :email, class: 'form-control' %>
  </div>
    </br>
  <div class="form-group">
    <button type="submit", class="btn btn-primary btn-sm">Creer</button>
  </div>
<% end %>
